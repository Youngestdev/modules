module web.front.elements

class Div

	singleDiv = "<div> "
	singleDivStyle = 'style="'
	
	block setBackground(icolor)
		singleDivStyle += 'background:icolor; '
	
	block toString()
		singleDiv += singleDivStyle+'" </div>'
		return singleDiv