module web.front.elements

call "Div.sim" ? web.front.elements

class Table : Div

	elementName = "table"
	init(elementName)
	
	block Table(id)
		setId(id)
	
	block setSize(height, width)
		elementHeight = height
		elementWidth = width
		elementStyle += 'height:'+elementHeight+';width:'+elementWidth+';'
	
	