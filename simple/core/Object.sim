module simple.core

call "simple/security/Hash.sim"
call "Types.sim"
call "BaseFields.sim"

class Object 

	objectName = "Object"

	block toString()
		return "[simple.core.Object:"+getHashCode()+"]"
		
	block getHashCode()
		return hash32(objectName)

	block equals(compare)
		#empty for now
		
	block getType()
		return objectName
		
	block getTypeCode
		return OBJECT_TYPE