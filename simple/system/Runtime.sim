module simple.system

call "System.sim"

Runtime = new Runtime

class Runtime

	block executeWithParameter(file, param)
		exe = '"'+file+'"' + " "+param
		__exec(exe)
		
	block execute(exe)
		__exec('"'+exe+'"')
